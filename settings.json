{
        "workbench.colorTheme": "Community Material Theme Ocean",
        "editor.fontFamily": "'Fira Code iScript','Courier New', monospace",

        "editor.fontSize": 18,
        "editor.fontLigatures": true,
        "editor.lineHeight": 25,
        "editor.rulers": [80],
        "editor.minimap.enabled": false,
        "editor.letterSpacing": 0.5,
        "files.trimTrailingWhitespace": true,
        "editor.fontWeight": "400",
        "prettier.printWidth":300,
        "editor.cursorStyle": "block",
        "editor.cursorWidth": 5,
        "editor.cursorBlinking": "solid",
        "editor.wordWrap": "off",
        "git.decorations.enabled": false,
        // Very important: Install this plugin: https://github.com/be5invis/vscode-custom-css
        // you'll need to change this to an absolute path on your computer
        "vscode_custom_css.imports": [
          "/Volumes/Boba Fett/Users/todd/.vscodestyles.css"
        ],
        "editor.renderWhitespace": "all",
        "liveSassCompile.settings.formats":[
          // You can add more
          {
              "format": "compressed",
              "extensionName": ".min.css",
              "savePath": "~/../css/dist/"
          }
        ],
        "files.associations": {
          "*.js": "javascriptreact"
        },
        "workbench.activityBar.visible": true,
        "sync.gist": "bfd7b1ccd703759188f27facbcb1ee4a",
        "sync.quietSync": true,
        "[javascriptreact]": {
          "editor.defaultFormatter": "esbenp.prettier-vscode"
        },
        "prettier.useTabs": true,
        "prettier.tabWidth": 4,
        // "[html]": {
        //   "editor.defaultFormatter": "esbenp.prettier-vscode"
        // },
          "editor.tokenColorCustomizations": {
          "textMateRules": [
            {
              "scope": "meta.object-literal.key",
              "settings": {
                "foreground": "#5ab1f8"
            }
          }
          ]
      },
      "beautify.wrap_attributes": "preserve-aligned",
      "beautify.wrap_attributes_indent_size": false,
      "[scss]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "workbench.startupEditor": "newUntitledFile",
      "html.format.wrapLineLength": 0,
      "[html]": {
        "editor.defaultFormatter": "HookyQR.beautify"
      },
      "turboConsoleLog.logMessagePrefix": "",
      "scm.diffDecorationsGutterWidth": 4,
    "editor.detectIndentation": false,
    "window.zoomLevel": 0,
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "zenMode.centerLayout": false,
    "zenMode.hideLineNumbers": false,
    "task.slowProviderWarning": [
        "typescript"
    ],
    "workbench.iconTheme": "material-icon-theme",
    "[javascript]": {
        "editor.defaultFormatter": "bysabi.prettier-vscode-semistandard"
    },
    "liveServer.settings.donotShowInfoMsg": true,
    // "emmet.syntaxProfiles": {
    //     "html": {
    //        "tag_nl": false
    //     }
    //   }
    "workbench.colorCustomizations": {
        "[Material Theme Palenight High Contrast]": {
            "editor.background": "#0a1021",
        }
    }

}
